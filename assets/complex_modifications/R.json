{
  "title": "R",
  "rules": [
    {
      "description": "R : C-h",
      "manipulators": [ {
        "type": "basic",
        "from": { "key_code": "h", "modifiers": { "mandatory": [ "control" ]} },
        "to": [{"key_code": "delete_or_backspace"}],
        "conditions": [ {
          "type": "frontmost_application_if",
          "bundle_identifiers": [ "^org.R-project.R$" ]
        } ]
      } ]
    }
  ]
}
