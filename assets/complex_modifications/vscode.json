{
  "title": "vscode",
  "rules": [
    {
      "description": "vscode - <c-n> to down",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": { "mandatory": ["control"] }
          },
          "to": [{ "key_code": "down_arrow" }],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": ["^com\\.microsoft\\.VSCode$"]
            }
          ]
        }
      ]
    }
  ]
}
